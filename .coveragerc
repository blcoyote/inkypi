[run]
source = core,utils,rendering,display
omit = 
    */tests/*
    */stubs/*
    */__pycache__/*
    */venv/*
    clear_screen.py

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    raise AssertionError
    raise NotImplementedError
    if __name__ == .__main__.:
    if TYPE_CHECKING:
    @abstractmethod
